{
    "schemaVersion": 1,
    "id": "my_fabric_mod",
    "version": "${version}",
    "name": "My Fabric Mod",
    "description": "This is an example description! Tell everyone what your mod is about!",
    "authors": [
        "Enzo"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/fabricmc/fabric-example-mod"
    },
    "license": "CC0-1.0",
    "icon": "assets/my_fabric_mod/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.enzo.mod.MyFabricMod"
        ],
        "client": [
            "com.enzo.mod.MyFabricModClient"
        ]
    },
    "mixins": [
        "my_fabric_mod.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "minecraft": "~1.20.1",
        "java": ">=17",
        "fabric-api": "*"
    },
    "suggests": {
        "another-mod": "*"
    }
}